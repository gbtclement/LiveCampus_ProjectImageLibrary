{% block title %}Statistiques images{% endblock %}

{% block body %}
<h2>Top 20 des images les plus téléchargées</h2>
<table>
    <thead>
        <tr>
            <th>Image</th>
            <th>Url</th>
        </tr>
    </thead>
    <tbody>
    {% for image in images %}
        <tr>
            <td>{{ image.name }}</td>
            <td>{{ image.url }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>
{% endblock %}
